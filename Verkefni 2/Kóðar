#include <LiquidCrystal.h>

LiquidCrystal lcd(12, 11, 5, 4, 3, 2);

void setup() {
  lcd.begin(0, 16);
  lcd.cursor();
  String nafn="Jakub " " Dembowski";
  lcd.print(nafn[0]);
  delay(600);
  lcd.print(nafn[1]);
  delay(600);
  lcd.print(nafn[2]);
  delay(600);
  lcd.print(nafn[3]);
  delay(600);
  lcd.print(nafn[4]);
  delay(600);
  lcd.print(nafn[6]);
  delay(600);
  lcd.print(nafn[7]);
  delay(600);
  lcd.print(nafn[8]);
  delay(600);
  lcd.print(nafn[9]);
  delay(600);
  lcd.print(nafn[10]);
  delay(600);
  lcd.print(nafn[11]);
  delay(600);
  lcd.print(nafn[12]);
  delay(600);
  lcd.print(nafn[13]);
  delay(600);
  lcd.print(nafn[14]);
  delay(600);
  lcd.print(nafn[15]);
  
}

void loop() {
  lcd.setCursor(0, 1);
  lcd.print("Tolvubraut");
}
